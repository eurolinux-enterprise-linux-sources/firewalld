 delete mode 100644 config/services/cockpit.xml

diff --git a/config/Makefile.am b/config/Makefile.am
index 7048d2ee5c0b..0a3e228bff74 100644
--- a/config/Makefile.am
+++ b/config/Makefile.am
@@ -130,7 +130,6 @@ CONFIG_FILES = \
